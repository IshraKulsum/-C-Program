// AGE CHECKIG BOOT 
#include <stdio.h>

int main() {
    int a;
    printf("Please Enter your Age:");
    scanf("%d", &a );
    
    if( a > 18){
        printf("Your an Adult \n");
        printf("Your Eligible to Vote\n");
    }else{
        printf("Your are underage\n");
        printf("Your are ot eligible to Vote\n");
    }
    printf("Thank you\n");

    return 0;
}
/*
Please Enter your Age:09
Your are underage
Your are ot eligible to Vote
Thank you


=== Code Execution Successful ===

Please Enter your Age:78
Your an Adult 
Your Eligible to Vote
Thank you


=== Code Execution Successful ===
*/

// AGE CHECKIG BOOT - usinf ternary operator(SINGLE LINE)
#include <stdio.h>

int main() {
    int a;
    printf("Please Enter your Age:");
    scanf("%d", &a );
    
     a >= 18? printf("Your an Adult \n You can Vote\n") : printf("Your are underage\n Noteligible to vote\n");

    printf("Thank you\n");

    return 0;
}
/*
OUTPUT:
Please Enter your Age:34
Your an Adult 
 You can Vote
Thank you


=== Code Execution Successful ===
Please Enter your Age:16
Your are underage
 Noteligible to vote
Thank you


=== Code Execution Successful ===
*/

// For Loop to print 0 to 10
#include <stdio.h>

int main() {
    for(int i = 0; i<= 10; i++){
        printf("%d \n", i);
    }

    return 0;
}
/* 
