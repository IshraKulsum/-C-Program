// Sum of 2 no's

# include <stdio.h>
 int main(){
    int a,b ;
    printf("1st no a:");
    scanf("%d",&a);
    printf("2nd no b:");
    scanf("%d",&b);
     printf(" summing both we get: %d ", a+b);
    return 0;
 }

/*
OUTPUT:
Enter you Interger here! : 9
integer = 9
float = 10.500000
character = a


=== Code Execution Successful ===
*/

// PRINT TRUE IF A NO IS GREATER THAN 9 AND LESS THAN 100

#include <stdio.h>

int main() {
    int a;
    printf("Enter your no:");
    scanf("%d", &a );
    printf(" %d", a> 9 && a < 100);


    return 0;
}
//OUTPUT:
/* 
Enter your no:89
