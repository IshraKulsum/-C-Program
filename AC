// For Loop to Float
#include <stdio.h>

int main() {
    for (float i = 0.1; i<= 0.9; i += 0.1 ){
        printf("%f \n", i);
    }

    return 0;
}
/*
0.100000 
0.200000 
0.300000 
0.400000 
0.500000 
0.600000 
0.700000 
0.800000 


=== Code Execution Successful ===
*/
// For while to Float
#include <stdio.h>

int main() {
  float i = 0.1;
  while(i<= 0.9){
        printf("%f \n", i);
        i += 0.1;
    }

    return 0;
}
/*
0.100000 
0.200000 
0.300000 
0.400000 
0.500000 
0.600000 
0.700000 
0.800000 


=== Code Execution Successful ===

*/
// For while loop applying on char 
#include <stdio.h>

int main() {
  char i = 'f';
  while(i<= 'v'){
        printf("%c \n", i);
        i++;
    }

    return 0;
}
/*
