// For do while loop applying on int 
#include <stdio.h>

int main() {
  int i = 1;
  do{
        printf("%d \n", i++);
        i++;
    }while(i<= 5);

    return 0;
}
/*
1 
3 
5 


=== Code Execution Successful ===
*/
